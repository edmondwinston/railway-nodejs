<h1>Login</h1>

<form action="/api/v1/account/authenticate" method="POST">
  <div>
    <label for="email">Email:</label>
    <input type="email" name="email" required />
  </div>

  <div>
    <label for="password">Password:</label>
    <input type="password" name="password" required />
  </div>
  <button>Login</button>
</form>
